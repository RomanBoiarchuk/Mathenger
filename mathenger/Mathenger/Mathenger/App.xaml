<Application x:Class="Mathenger.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Mathenger">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ui\styles\Colors.xaml"/>
        <ResourceDictionary Source="ui\styles\Fonts.xaml"/>
        <ResourceDictionary Source="ui\styles\Buttons.xaml"/>
        <ResourceDictionary Source="ui\styles\Texts.xaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
