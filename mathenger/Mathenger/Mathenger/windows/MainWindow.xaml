<Window x:Class="Mathenger.windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mathenger"
        mc:Ignorable="d"
        Name="mainWindow"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Margin="5" Text="{Binding Account.FirstName}"/>
            <TextBlock Margin="5" Text="{Binding Account.LastName}"/>
        </StackPanel>
        <Button Name="SignOutButton" Click="SignOutButton_OnClick">Sign Out</Button>
    </StackPanel>
</Window>
